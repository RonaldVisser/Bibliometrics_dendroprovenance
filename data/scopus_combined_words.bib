@ARTICLE{Kuhl2023,
	author = {Kuhl, Eileen and Zang, Christian and Esper, Jan and Riechelmann, Dana F. C. and Büntgen, Ulf and Briesch, Martin and Reinig, Frederick and Römer, Philipp and Konter, Oliver and Schmidhalter, Martin and Hartl, Claudia},
	title = {Using machine learning on tree-ring data to determine the geographical provenance of historical construction timbers},
	year = {2023},
	journal = {Ecosphere},
	volume = {14},
	number = {3},
	doi = {10.1002/ecs2.4453},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85152662867&doi=10.1002%2fecs2.4453&partnerID=40&md5=d4aa6896d82ea5ab947f530202b2bb52},
	affiliations = {Department of Geography, Johannes Gutenberg University, Mainz, Germany; Department of Forestry, University of Applied Science Weihenstephan-Triesdorf, Freising, Germany; Global Change Research Centre (CzechGlobe), Brno, Czech Republic; Institute for Geosciences, Johannes Gutenberg University, Mainz, Germany; Department of Geography, University of Cambridge, Cambridge, United Kingdom; Swiss Federal Research Institute (WSL), Birmensdorf, Switzerland; Department of Geography, Masaryk University, Brno, Czech Republic; Department of Information Systems and Business Administration, Johannes Gutenberg University, Mainz, Germany; DENDROSUISSE - Labor für Dendrochronologie, Brig, Switzerland; Nature Rings - Environmental Research and Education, Mainz, Germany},
	abstract = {Dendroclimatology offers the unique opportunity to reconstruct past climate at annual resolution and wood from historical buildings can be used to extend such information back in time up to several millennia. However, the varying and often unclear origin of timbers affects the climate sensitivity of individual tree-ring samples. Here, we compare tree-ring width and density of 143 living larch (Larix decidua Mill.) trees at seven sites along an elevational transect from 1400 to 2200 m asl and 99 historical tree-ring series to parametrize state-of-the-art classification models for the European Alps. To achieve geographical provenance of the historical series, nine different supervised machine learning algorithms are trained and tested in their capability to solve our classification problem. Based on this assessment, we consider a tree-ring density-based and a tree-ring width-based dataset for model building. For each of these datasets, a general not species-related model and a larch-specific model including the cyclic larch budmoth influence are built. From the nine tested machine learning algorithms, Extreme Gradient Boosting showed the best performance. The density-based models outperform the ring-width models with the larch-specific density model reaching the highest skill (f1 score = 0.8). The performance metrics reveal that the larch-specific density model also performs best within individual sites and particularly in sites above 2000 m asl, which show the highest temperature sensitivities. The application of the specific density model for larch allows the historical series to be assigned with high confidence to a particular elevation within the valley. The procedure can be applied to other provenance studies using multiple tree growth characteristics. The novel approach of building machine learning models based on tree-ring density features allows to omit a common period between reference and historical data for finding the provenance of relict wood and will therefore help to improve millennium-length climate reconstructions. © 2023 The Authors. Ecosphere published by Wiley Periodicals LLC on behalf of The Ecological Society of America.},
	author_keywords = {artificial intelligence; dendrochronology; dendroprovenancing; European Alps; Extreme Gradient Boosting; Larix decidua; tree-ring density; tree-ring width},
	correspondence_address = {E. Kuhl; Department of Geography, Johannes Gutenberg University, Mainz, Germany; email: eikuhl@uni-mainz.de},
	publisher = {John Wiley and Sons Inc},
	issn = {21508925},
	language = {English},
	abbrev_source_title = {Ecosphere},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 1; All Open Access, Gold Open Access, Green Open Access}
}

@BOOK{Bernabei2018111,
	author = {Bernabei, Mauro and Martinelli, Nicoletta and Cherubini, Paolo},
	title = {Tree-Ring Analysis on Wooden Artifacts: What Can It Tell Us?},
	year = {2018},
	journal = {Nanotechnologies and Nanomaterials for Diagnostic, Conservation and Restoration of Cultural Heritage},
	pages = {111 – 125},
	doi = {10.1016/B978-0-12-813910-3.00006-9},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081529920&doi=10.1016%2fB978-0-12-813910-3.00006-9&partnerID=40&md5=72f8d9740c17e7996bb040483819495d},
	affiliations = {CNR-IVALSA, Trees and Timber Institute, San Michele a/Adige (TN), Italy; Laboratorio Dendrodata, Verona, Italy; WSL Swiss Federal Institute for Forest, Snow and Landscape Research, Birmensdorf, Switzerland},
	abstract = {Wood has been used by mankind since prehistory; therefore the presence of wood in cultural heritage is huge. The dendrochronological dating is considered one of the principal techniques for the study of wood of historical and architectural interest. When the traces left by the history make a correct reading of the context impossible, the results given by dendrochronology are often the only certain data useful as a reference. Wood species identification and tree-ring analysis allow far more than just dating: often it is possible to identify the provenance of the woods, closely studying some technical aspects, detecting major or minor interventions of restoration, substitution, and reuse, and, in some fields of study, verifying the attribution of the artifact to a certain artist or school. The power of dendrochronology is based on the rigorous scientific approach and the complete independence from other stylistic or historical considerations. © 2019 Copyright © 2019 Elsevier Inc. All rights reserved..},
	author_keywords = {Dendrochronology; Dendroprovenancing; Tree-ring analysis; Wood dating; Wood properties},
	publisher = {Elsevier},
	isbn = {978-012813911-0; 978-012813910-3},
	language = {English},
	abbrev_source_title = {Nanotechnologies and Nanomater. for Diagn., Conserv. and Restor. of Cult. Herit.},
	type = {Book chapter},
	publication_stage = {Final},
