	keywords = {Cantabria; Spain; Quercus; Quercus faginea; Quercus petraea; Quercus pyrenaica; Quercus robur; anatomy; climate effect; deciduous tree; dendroecology; multivariate analysis; phytogeography; precision; timber; tree ring; wood quality},
	correspondence_address = {L. Akhmetzyanov; Forest Ecology and Forest Management Group, Wageningen University & Research, Wageningen, Netherlands; email: linar.akhmetzyanov@wur.nl},
