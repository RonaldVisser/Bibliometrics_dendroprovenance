	correspondence_address = {A. Christopoulou; Centre for Research and Conservation of Cultural Heritage, Faculty of Fine Arts, Nicolaus Copernicus University, Toruń, 87-100, Poland; email: anchristo@umk.pl},
	publisher = {MDPI AG},
	issn = {19994907},
	language = {English},
	abbrev_source_title = {Forests},
	type = {Article},
	publication_stage = {Final},
