@article{ WOS:000331393000010,
Author = {Haneca, Kristof and Daly, Aoife},
Title = {Tree-Rings, Timbers and Trees: a dendrochronological survey of the
   14th-century cog, Doel 1},
Journal = {INTERNATIONAL JOURNAL OF NAUTICAL ARCHAEOLOGY},
Year = {2014},
Volume = {43},
Number = {1},
Pages = {87-102},
Month = {MAR},
Abstract = {In 2000, the remains of a cog, Doel 1, were found in Doel, Belgium. Wood
   species identification of all ship timbers and smaller elements was
   performed. European oak was the dominant species, followed by alder that
   was used for the fairings. In total 150 ring-width series were recorded.
   The construction date was set at AD 1325/26 and the timbers proved to
   originate from forests along the rivers Elbe and Weser. For the bottom
   strakes a strict symmetrical layout was observed. The keel plank was
   hewn from a trunk with a slightly earlier felling date. Repairs were
   performed with high-quality boards, some with a southern Baltic
   provenance.},
