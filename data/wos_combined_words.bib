@article{ WOS:000713472500001,
Author = {Gardiner, Mark},
Title = {Twelfth-Century Timbers From Sixhills, Lincolnshire, and a Review of
   Medieval Stave Construction in England},
Journal = {VERNACULAR ARCHITECTURE},
