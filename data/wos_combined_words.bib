@article{ WOS:000340522000002,
Author = {Nayling, Nigel and Susperregi, Josue},
Title = {Iberian Dendrochronology and the Newport Medieval Ship},
Journal = {INTERNATIONAL JOURNAL OF NAUTICAL ARCHAEOLOGY},
Year = {2014},
Volume = {43},
Number = {2},
Pages = {279-291},
Month = {SEP},
Abstract = {At the time of the discovery of the Newport medieval ship, it proved
   impossible to date the timbers used in its original construction through
   dendrochronology. Associated British timbers and artefacts provided
   dating to the mid 15th century, with the latter pointing to strong
   Iberian connections. The development of regional oak ring-width
   chronologies in the Basque Country, and their extension back in time to
   overlap with the ring-width mean developed for the Newport Ship, has
   allowed absolute dating and provenance of timbers used in its original
   construction. Further research is required to clarify the geographical
   source of the timbers and to develop a high-resolution network of
   tree-ring data for the region. (C) 2013 The Authors},
