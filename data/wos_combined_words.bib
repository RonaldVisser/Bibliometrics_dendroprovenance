@article{ WOS:000884246900001,
Author = {Belasus, Mike and Daly, Aoife},
Title = {Timber Resources for the `Bremen Cog'},
Journal = {INTERNATIONAL JOURNAL OF NAUTICAL ARCHAEOLOGY},
Year = {2022},
Volume = {51},
Number = {2},
Pages = {207-231},
Month = {2022 JUL 3},
Abstract = {In 2016, about 40 years after the first dendrochronological
   investigation of the `Bremen Cog', a new comprehensive analysis was
   carried out, which included an examination of timber quality. The new
   dating of 55 samples matches with the 1960s' results. However, their
   provenance was established in the direct hinterlands of Bremen. The
   anatomical features of the vessel's timber reflect heavily exploited
   forests in the vicinity of a late medieval town. They comply with the
   forest history of the western part of the North European Plain and can
   be considered critical for quality in shipbuilding.},
