@article{ WOS:000487961100003,
Author = {Gut, Urs and Arvai, Matyas and Bijak, Szymon and Julio Camarero, J. and
   Cedro, Anna and Cruz-Garcia, Roberto and Garamszegi, Balazs and
   Hacket-Pain, Andrew and Hevia, Andrea and Huang, Weiwei and
